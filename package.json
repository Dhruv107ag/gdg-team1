{
  "name": "focusez-extension",
  "displayName": "Focusez",
  "version": "0.0.1",
  "description": "Browser extension for nerds to help them focus",
  "author": "prashant",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "tabs",
      "bookmarks",
      "identity",
      "notifications",
      "activeTab"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "chrome_url_overrides": {
      "newtab": "tabs/newtab.html"
    }
  }
}